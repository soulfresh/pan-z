{
  "name": "@thesoulfresh/pan-z",
  "version": "1.2.0",
  "private": false,
  "license": "MIT",
  "repository": "https://github.com/soulfresh/pan-z",
  "keywords": [
    "pan",
    "zoom",
    "dom",
    "svg",
    "image",
    "mobile",
    "browser"
  ],
  "main": "lib/lib.min.js",
  "browser": "lib/lib.min.js",
  "files": [
    "lib/*",
    "src/PanZ.js",
    "src/index.js"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "start": "yarn storybook",
    "clean": "rimraf lib coverage storybook-static",
    "prebuild": "yarn clean",
    "build": "yarn prebuild && webpack --env production",
    "lint": "yarn eslint",
    "lint-fix": "yarn eslint-fix",
    "eslint": "eslint 'src/**/*.js'",
    "eslint-fix": "yarn eslint --fix",
    "test": "karma start",
    "test-debug": "karma start --log-level debug --jasmine-timeout=300000 --browsers=Chrome",
    "test-ci": "karma start --single-run --no-auto-watch --browsers=Chrome,Firefox",
    "validate": "yarn lint-fix",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "yarn clean && build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "prepublishOnly": "yarn build",
    "release": "./scripts/release"
  },
  "dependencies": {
    "@thesoulfresh/pan-zoom": "^4.0.0",
    "eventemitter3": "^4.0.0",
    "lodash.debounce": "^4.0.8",
    "object-fit-math": "^1.0.0",
    "resize-observer-polyfill": "^1.5.1"
  },
  "devDependencies": {
    "@babel/cli": "7.10.5",
    "@babel/core": "7.11.1",
    "@babel/preset-env": "7.11.0",
    "@storybook/addon-actions": "^6.0.4",
    "@storybook/addon-docs": "^6.0.4",
    "@storybook/addon-links": "^6.0.4",
    "@storybook/addons": "^6.0.4",
    "@storybook/cli": "^6.0.4",
    "@storybook/react": "^6.0.4",
    "@storybook/storybook-deployer": "^2.8.6",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.1.0",
    "babel-plugin-add-module-exports": "^1.0.0",
    "babel-plugin-istanbul": "^6.0.0",
    "babel-plugin-transform-es2015-modules-umd": "^6.24.1",
    "chalk": "^4.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "eslint": "^7.6.0",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-compat": "^3.8.0",
    "eslint-plugin-editorconfig": "^2.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jasmine": "^4.1.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.0.1",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "jasmine-core": "^3.6.0",
    "karma": "^5.1.1",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-firefox-launcher": "^1.3.0",
    "karma-jasmine": "^4.0.0",
    "karma-junit-reporter": "^2.0.1",
    "karma-mocha-reporter": "^2.2.5",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-spec-reporter": "^0.0.32",
    "karma-webpack": "^4.0.2",
    "nodemon": "^2.0.4",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-is": "^16.13.1",
    "rimraf": "^3.0.2",
    "terser-webpack-plugin": "^4.1.0",
    "webpack": "^4.44.1",
    "webpack-cli": "^3.3.12",
    "webpack-merge": "^5.1.1"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
